abstract MobileController(HUDButton) {

	final BoundedFloat offset;
	boolean active;
	boolean horizontal;
	int screenHAlign;
	int screenVAlign;
	int hAlign;
	int vAlign;
	float defX;
	float defY;
	float width;
	float height;
	final BattlePlayer player;

	new(BattlePlayer playerScript, MobileRoleButton roleButton) {}

	boolean equals(Object arg0) {}

	String toString() {}

	int hashCode() {}

	void update(float delta) {}

	boolean isHidden() {}

	float getY() {}

	void draw(CustomSpriteBatch batch, float xOfs, float yOfs) {}

	float getX() {}

	void event(GameEvent event) {}

	void button(ButtonEvent trigger) {}

	boolean touch(TouchEvent trigger) {}

	float calcY() {}

	float calcX() {}

	float getMove() {}

	boolean finishedMoving() {}

	void finishMoving() {}

	boolean increaseOffset() {}

	boolean isTouching(float x, float y) {}

	void addButton(MobileButton button) {}

	boolean clickPanel(TouchEvent trigger) {}

}
