abstract MobileRoleButton(Object) : MobileButton, InputListener {

	new(DefaultBattlePlayer script, boolean vertical) {}

	boolean equals(Object arg0) {}

	String toString() {}

	int hashCode() {}

	void controllerConnected(Controller controller) {}

	void controllerDisconnected(Controller controller) {}

	boolean triggerType(int character) {}

	int getInputPriority() {}

	boolean triggerTouch(TouchEvent event) {}

	boolean triggerButton(ButtonEvent event) {}

	boolean addButtonTips(ButtonTipList tips) {}

	boolean keyDown(int keycode) {}

	boolean keyTyped(char character) {}

	boolean keyUp(int keycode) {}

	void update(float delta) {}

	boolean isHidden() {}

	void draw(CustomSpriteBatch batch, float xOfs, float yOfs) {}

	boolean touch(TouchEvent trigger) {}

	void button(ButtonEvent trigger) {}

	abstract float getY();

	abstract float getX();

	abstract float getWidth();

	abstract float getHeight();

	void paint(Painter painter, VocSet vocs, boolean small) {}

	float getButtonSize() {}

	boolean isTouching(float positionX, float positionY) {}

	float getSmallButtonSize() {}

}
