abstract MinimapMarker(Object) {

	final DrawableImage image;
	final Color color;
	int hAlign;
	int vAlign;

	new(DrawableImage image, Color color) {}

	boolean equals(Object arg0) {}

	String toString() {}

	int hashCode() {}

	boolean remove() {}

	void update(float delta) {}

	void draw(CustomSpriteBatch batch, float x, float y) {}

	abstract float getY();

	abstract float getX();

}
