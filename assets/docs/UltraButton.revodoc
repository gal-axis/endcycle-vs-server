abstract UltraButton(MobileVocButton) {

	MenuThemeDrawable background;
	final DefaultBattlePlayer player;
	final int index;
	final DrawableImage energy;

	new(DefaultBattlePlayer player) {}

	boolean equals(Object arg0) {}

	String toString() {}

	int hashCode() {}

	boolean priority() {}

	void controllerConnected(Controller controller) {}

	void controllerDisconnected(Controller controller) {}

	boolean triggerType(int character) {}

	boolean triggerButton(ButtonEvent event) {}

	boolean keyUp(int keycode) {}

	boolean keyDown(int keycode) {}

	boolean addButtonTips(ButtonTipList tips) {}

	boolean keyTyped(char character) {}

	boolean triggerTouch(TouchEvent event) {}

	void update(float delta) {}

	boolean isHidden() {}

	void draw(CustomSpriteBatch batch, float xOfs, float yOfs) {}

	void button(ButtonEvent trigger) {}

	boolean touch(TouchEvent trigger) {}

	abstract float getX();

	abstract float getY();

	VocEntry getVoc(VocSet set) {}

	CColor getColor(VocEntry voc) {}

	KeyCode getCode() {}

	boolean isTouching(int posX, int posY) {}

	void drawVoc(CustomSpriteBatch batch, VocEntry voc, float x, float y, float scale, Color color) {}

	void drawButton(CustomSpriteBatch batch, float scale, float sizeX, float sizeY, float x, float y) {}

	abstract float getVocSize();

	abstract float getSizeY();

	abstract float getSizeX();

}
