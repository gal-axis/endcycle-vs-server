abstract Mover(Object) {

	final BattleEntity entity;

	new(BattleEntity parent) {}

	boolean equals(Object obj) {}

	String toString() {}

	int hashCode() {}

	abstract void update(float delta);

	abstract void init(int x, int y, float speed);

	abstract void start();

	abstract boolean over();

	abstract void cancel();

	Battle getBattle() {}

}
