abstract HUDButton(Object) {

	final BoundedFloat offset;
	boolean active;
	boolean horizontal;
	int screenHAlign;
	int screenVAlign;
	int hAlign;
	int vAlign;
	float defX;
	float defY;
	float width;
	float height;

	new(float x, float y, float width, float height, boolean horizontal) {}

	boolean equals(Object arg0) {}

	String toString() {}

	int hashCode() {}

	void update(float delta) {}

	boolean isHidden() {}

	abstract void draw(CustomSpriteBatch batch, float xOfs, float yOfs);

	float getX() {}

	float getY() {}

	float calcX() {}

	float calcY() {}

	void event(GameEvent event) {}

	void button(ButtonEvent trigger) {}

	boolean touch(TouchEvent trigger) {}

	boolean finishedMoving() {}

	boolean isTouching(float x, float y) {}

	void finishMoving() {}

	float getMove() {}

	boolean increaseOffset() {}

}
